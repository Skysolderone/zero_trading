syntax = "v1"

type Base {
	Code    int    `json:"code"`
	Message string `json:"message"`
}
type BaseResponse {
	Base
	Data interface{} `json:"data"`
}

type BaseListResponse {
	Base
	Data  interface{} `json:"data"`
	Total int         `json:"total"`
}

type BasePageResponse {
	Base
	Data     interface{} `json:"data"`
	Total    int         `json:"total"`
	Page     int         `json:"page"`
	PageSize int         `json:"page_size"`
}
